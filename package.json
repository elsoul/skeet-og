    "@types/read": "^0.0.29",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "nodemon": "2.0.20",
    "npm-check-updates": "16.4.1",
    "prettier": "2.7.1",
    "babel-loader": "9.1.2",
    "esbuild": "0.17.7",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
